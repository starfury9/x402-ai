{
  "name": "agentpay",
  "version": "1.0.0",
  "description": "AgentPay - Pay-Per-Use AI Agent Marketplace powered by x402-stacks",
  "scripts": {
    "install:all": "cd server && npm install && cd ../client && npm install",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "x402",
    "x402-stacks",
    "stacks",
    "bitcoin",
    "micropayments",
    "ai-agents",
    "marketplace",
    "http-402",
    "hackathon"
  ],
  "license": "MIT"
}
